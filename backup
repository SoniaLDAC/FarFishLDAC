library(shiny)
library(rsconnect)

#lectura de datos
senegal <- read.csv("senegal.csv", sep=",")
library(readr)
senegal2 <- read.csv("senegal2.csv", sep=";", fileEncoding="latin1")
SENEGAL3 <- read.csv("SENEGAL3.csv", sep=";", fileEncoding="latin1")
mauritania <- read.csv("mauritania.csv", sep=";", fileEncoding="latin1")
mauritania2 <- read.csv("mauritania2.csv", sep=";", fileEncoding="latin1")

senegalporpais <- senegal2
senegalperyear <- SENEGAL3
mauritaniaperflag <- mauritania2
mauritaniaperyear <- mauritania

