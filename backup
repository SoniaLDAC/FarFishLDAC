#Nombrar outputs
#textOutput(outputId = "textflag"),
# Define server logic required to draw a histogram
server <- function(input, output) {
  
  formulaText <- reactive({
    paste(input$senflag, "~ YEAR")
  })
  
  # Return the formula text for printing as a caption ----
  output$caption <- renderText({
    formulaText()
  })
  # Generate a plot of the requested variable against mpg ----
  # and only exclude outliers if requested
  output$plotsenflag <- renderPlot({
    plot(as.formula(formulaText()),
         data = senegalporpais,
         col = "#75AADB", pch = 19, type = "o") #quitar type "" para volver a grÃ¡fico de puntos separados
  })
  
  #HISTOGRAMA DE CADA AÃO POR PAIS
  formulaText2 <- reactive({
    paste(input$senyear, "~ PAIS")
  })
  
  # GENERAR HISTGRAMA POR AÃO
  output$plotsenyear <- renderPlot({
    barplot(as.formula(formulaText2()),
            data = senegalperyear,
            col = rainbow(5), pch = 19, las=2, cex.names=.9)
  }) 
  
  
  formulaText3 <- reactive({
    paste(input$mauflag, "~ YEAR")
  })
  
  
  # Generate a plot of the requested variable against mpg ----
  # and only exclude outliers if requested
  output$plotmauflag <- renderPlot({
    plot(as.formula(formulaText3()),
         data = mauritaniaperflag,
         col = "#75AADB", pch = 19, type = "o") #quitar type "" para volver a grÃ¡fico de puntos separados
  })
  
  #HISTOGRAMA DE CADA AÃO POR PAIS
  
  formulaText4 <- reactive({
    paste(input$mauyear, "~ PAIS")
  })
  
  # GENERAR HISTOGRAMA POR AÃO
  output$plotmauyear <- renderPlot({
    barplot(as.formula(formulaText4()),
            data = mauritaniaperyear,
            col = rainbow(5), pch = 19, las=2, cex.names=.9)
  })
  
  
}

# Run the application 
shinyApp(ui = ui, server = server)


